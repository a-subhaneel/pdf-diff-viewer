    "url": "https://github.com/a-subhaneel/pdf-diff-viewer.git"
  },
  "bugs": {
    "url": "https://github.com/a-subhaneel/pdf-diff-viewer/issues"
  },
  "homepage": "https://github.com/a-subhaneel/pdf-diff-viewer#readme",
  "files": [
    "src/",
    "public/",
    "bin/",
    "server.js",
    "README.md",
